#. type: SH
#: en/linkchecker.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNTAX"

# type: Plain text
#. type: Plain text
#: en/linkchecker.1:7
msgid "B<linkchecker> [I<options>] [I<file-or-url>]..."
msgstr "B<linkchecker> [I<Optionen>] [I<Datei-oder-URL>]..."

